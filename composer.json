{
    "name": "stringphp/database",
    "description": "MySql ORM for amphp-mysql",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "StringPhp\\Database\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Robert Snedeker",
            "email": "rsnedeker20@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "amphp/mysql": "^v3.0.0",
        "amphp/log": "^v2.0.0",
        "stringphp/utils": "^v0.0.2",
        "stringphp/validation": "^v0.0.1",
        "stringphp/models": "^v0.0.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.16"
    },
    "scripts": {
        "fix": "php-cs-fixer fix --using-cache=no",
        "fix:dry": "php-cs-fixer fix --using-cache=no --diff --dry-run"
    }
}
