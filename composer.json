{
    "name": "stringphp/database",
    "description": "StringPhp's amphp-mysql wrapper",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "StringPhp\\Database\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Robert Snedeker",
            "email": "rsnedeker20@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "amphp/mysql": "^v3.0.0",
        "amphp/log": "^v2.0.0",
        "stringphp/utils": "dev-main",
        "stringphp/validation": "dev-main",
        "stringphp/models": "dev-main"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.16"
    },
    "scripts": {
        "fix": "php-cs-fixer fix --using-cache=no",
        "fix:dry": "php-cs-fixer fix --using-cache=no --diff --dry-run"
    },
    "repositories": [{
        "type": "path",
        "url": "../**"
    }]
}
